pipeline{
    agent{label 'jenkins'}
    stages{
        stage('source-code-management'){
            steps{
                git 'https://github.com/mmumshad/simple-webapp.git'
            }
        }
        stage('ansible-playbook'){
            steps{
ansiblePlaybook become: true, 
disableHostKeyChecking: true, 
installation: 'Ansible', 
inventory: 'hosts', 
playbook: 'python.yaml', 
sudo: true
            }
        }
    }
}