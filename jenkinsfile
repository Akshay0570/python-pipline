pipeline{
    stages{
        stage('source code management'){
            steps{
                git 'https://github.com/Akshay0570/python-sample-vscode-flask-tutorial.git'
            }
        }
        stage('install dependencies'){
            steps{
                sh '''sudo apt update
python3-pip
python3-setuptools
python3-dev
build-essential
python3-pip'''
            }
        }
    }
}